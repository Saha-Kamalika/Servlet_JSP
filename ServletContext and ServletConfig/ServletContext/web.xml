<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
<servlet>
    <servlet-name>AddServlet</servlet-name>
    <servlet-class>com.example.AddServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>AddServlet</servlet-name>
    <url-pattern>/add</url-pattern>
</servlet-mapping>

<servlet>
    <servlet-name>SqServlet</servlet-name>
    <servlet-class>com.example.SqServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>SqServlet</servlet-name>
    <url-pattern>/sq</url-pattern>
</servlet-mapping>
<servlet>
    <servlet-name>MyServlet</servlet-name>
    <servlet-class>com.example.MyServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>MyServlet</servlet-name>
    <url-pattern>/Name</url-pattern>
</servlet-mapping>
<servlet-mapping>
    <servlet-name>MyServlet</servlet-name>
    <url-pattern>/Designation</url-pattern>
</servlet-mapping>
<context-param>
	<param-name>Name</param-name>
	<param-value>Kamalika</param-value>
</context-param>
<context-param>
	<param-name>Designation</param-name>
	<param-value>Employee</param-value>
</context-param>
</web-app>